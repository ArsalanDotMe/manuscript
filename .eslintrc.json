{
    "rules": {
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [1, "always-multiline"],
        "no-unused-vars": [0]
    },
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended"
}
